\pard\plain\ltrpar\ql\s27\itap0\tx360{\s27\f5\fs22\lang1027{\*\listtag0}\abinodiroverride\ltrch CREATE FUNCTION DNI(NUMERO INT) RETURNS VARCHAR(10) BEGIN DECLARE RESUL INT;DECLARE LETRA VARCHAR(10); SELECT MOD(NUMERO,23) INTO RESUL;SELECT LET INTO LETRA FROM dni WHERE NUM=RESUL; RETURN CONCAT(NUMERO,LETRA);END}\par}