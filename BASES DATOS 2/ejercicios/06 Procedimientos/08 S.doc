\pard\plain\ltrpar\ql\s27\intbl\itap1{\s27\f3\fs20\lang1027{\*\listtag0}\abinodiroverride\ltrch CREATE PROCEDURE EJ1(IN NOM VARCHAR(20),IN COGNOM VARCHAR(20), IN DATA DATE,IN TABLA VARCHAR(64)) BEGIN CASE TABLA WHEN 'professor' THEN INSERT INTO professor(nom,cognom,data_naixement) values (nom,cognom,data); when 'alumne' THEN INSERT INTO alumne(nom,cognom,data_naixement) values (nom,cognom,data); ELSE SELECT 'HAS INTRODUCIDO UNA TABLA NO VALIDA'; END CASE; END//}{\s27\f3\fs20\lang1027{\*\listtag0}\par}
